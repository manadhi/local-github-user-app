<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.udhipe.favoriteuserreader">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".app.FavUserApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Favorite User Reader"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FavoriteUser">
        <activity android:name=".userfavorite.UserFavoriteActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".userdetail.UserDetailActivity" />
    </application>

</manifest>